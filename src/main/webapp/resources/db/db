DROP TABLE REPORTCR CASCADE CONSTRAINTS;
CREATE TABLE REPORTCR( 
REPORTCR_NUM VARCHAR2(30) CONSTRAINT REPORTCR_NUM_pk PRIMARY key,
REPORTCR_CORPREVIEWNUM VARCHAR2(40) CONSTRAINT REPORTCR_CORPREVIEWNUM_fk REFERENCES CORPREVIEW(CORPREVIEW_NUM) ON DELETE CASCADE,
REPORTCR_USERNUM varchar2(30) CONSTRAINT REPORTCR_USERNUM_fk REFERENCES USER2(USER_NUM) ON DELETE CASCADE,
REPORTCR_CONTENT varchar2(2000),
REPORTCR_RESULT varchar2(20),
REPORTCR_ADMINNUM varchar2(20),
REPORTCR_REGDATE DATE,
REPORTCR_TYPE varchar2(20)
);
