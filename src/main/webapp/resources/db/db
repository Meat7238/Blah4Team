CREATE TABLE ADMIN2( 
ADMIN_NUM VARCHAR2(20) CONSTRAINT ADMIN_NUM_pk PRIMARY key, 
ADMIN_ID varchar2(20),
ADMIN_PASSWORD varchar2(20),
ADMIN_NAME varchar2(20),
ADMIN_EMAIL varchar2(20)
