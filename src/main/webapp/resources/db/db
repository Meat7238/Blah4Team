DROP TABLE USER2 CASCADE CONSTRAINTS;
DROP TABLE ADMIN2 CASCADE CONSTRAINTS;
DROP TABLE SET2 CASCADE CONSTRAINTS;
DROP TABLE CORP CASCADE CONSTRAINTS;
DROP TABLE NOTICE CASCADE CONSTRAINTS;
DROP TABLE PAYLIST CASCADE CONSTRAINTS;
DROP TABLE CORPREVIEW CASCADE CONSTRAINTS;
DROP TABLE REVIEWPAY CASCADE CONSTRAINTS;
DROP TABLE POST CASCADE CONSTRAINTS;
DROP TABLE POSTREVIEW CASCADE CONSTRAINTS;
DROP TABLE RECRUIT CASCADE CONSTRAINTS;
DROP TABLE REPORTCR CASCADE CONSTRAINTS;
DROP TABLE REPORTPR CASCADE CONSTRAINTS;
DROP TABLE REPORTP CASCADE CONSTRAINTS;
----------------------------------------------------------------------
CREATE TABLE USER2( 
USER_NUM VARCHAR2(30) CONSTRAINT USER_NUM_pk PRIMARY KEY , 
USER_ID varchar2(50),
USER_PASSWORD varchar2(50),
USER_NICK varchar2(50),
USER_POINT number,
USER_EMAIL varchar2(50),
USER_STARTDATE date,
USER_ENDDATE date,
USER_JOBGROUP varchar2(50),
USER_WORKSPACE varchar2(50)
);

insert into user2 
(USER_NUM, USER_ID, USER_PASSWORD, USER_NICK, USER_POINT, USER_EMAIL, USER_STARTDATE, USER_ENDDATE, USER_JOBGROUP, USER_WORKSPACE) 
values ('001', 'aaa', '1234', 'kosa', 3000, 'kosa@kosa.com', sysdate, sysdate, 'IT', '서울');
insert into user2 
(USER_NUM, USER_ID, USER_PASSWORD, USER_NICK, USER_POINT, USER_EMAIL, USER_STARTDATE, USER_ENDDATE, USER_JOBGROUP, USER_WORKSPACE) 
values ('002', 'bbb', '1234', 'kosa2', 2000, 'kosa2@kosa.com', sysdate, sysdate, '서비스', '고양');
insert into user2 
(USER_NUM, USER_ID, USER_PASSWORD, USER_NICK, USER_POINT, USER_EMAIL, USER_STARTDATE, USER_ENDDATE, USER_JOBGROUP, USER_WORKSPACE) 
values ('003', 'ccc', '1234', 'kosa3', 4000, 'kosa3@kosa.com', sysdate, sysdate, '증권', '제주');
insert into user2 
(USER_NUM, USER_ID, USER_PASSWORD, USER_NICK, USER_POINT, USER_EMAIL, USER_STARTDATE, USER_ENDDATE, USER_JOBGROUP, USER_WORKSPACE) 
values ('004', 'ddd', '1234', 'kosa4', 1000, 'kosa4@kosa.com', sysdate, sysdate, '부동산', '인천');
insert into user2 
(USER_NUM, USER_ID, USER_PASSWORD, USER_NICK, USER_POINT, USER_EMAIL, USER_STARTDATE, USER_ENDDATE, USER_JOBGROUP, USER_WORKSPACE) 
values ('005', 'eee', '1234', 'kosa5', 5000, 'kosa5@kosa.com', sysdate, sysdate, '디자이너', '춘천');

select * from user2;
commit;
