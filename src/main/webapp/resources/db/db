CREATE TABLE SET2( 
SET_NUM VARCHAR2(20) CONSTRAINT SET_NUM_pk PRIMARY key, 
SET_NAME varchar2(20),
SET_PRICE varchar2(20),
SET_POINT varchar2(20)
);
----------------------------------------------------------------------
CREATE TABLE CORP( 
CORP_NUM VARCHAR2(20) CONSTRAINT CORP_NUM_pk PRIMARY key, 
CORP_NAME varchar2(20),
CORP_ADDR varchar2(20),
CORP_CATEGORY varchar2(20),
CORP_MADEDATE varchar2(20),
CORP_EMPLCOUNT varchar2(20),
CORP_LOCATE varchar2(20),
CORP_PAY varchar2(20),
CORP_DETEAIL varchar2(20)
);
----------------------------------------------------------------------

CREATE TABLE NOTICE( 
NOTICE_NUM NUMBER CONSTRAINT NOTICE_NUM_pk PRIMARY key, 
NOTICE_TITLE varchar2(100),
NOTICE_CATEGORY varchar2(20),
NOTICE_CONTENT varchar2(4000),
NOTICE_ADMINNUM varchar2(20), 
NOTICE_REGDATE varchar2(20),
NOTICE_READCOUNT varchar2(20)
);

INSERT INTO notice VALUES(1,'怨듭��젣紐�','怨듭�移댄뀒怨좊━','怨듭��궡�슜','admin001',sysdate,'12');
